  "scripts": {
    "all": "run-z +z build,lint,test",
    "build": "run-z +z --then rollup --config ./rollup.config.js",
    "ci:all": "run-z all +test/--ci/--runInBand",
    "clean": "run-z +z --then shx rm -rf dist target",
    "lint": "run-z +z --then eslint . --ext .js,.ts",
    "site:publish": "run-z +z all --then gh-pages --dist dist --dotfiles",
    "start": "run-z +z build --then node -r esm ./build/server.js",
    "test": "run-z +z --then jest",
    "z": "run-z +build,+lint,+test"
  }
